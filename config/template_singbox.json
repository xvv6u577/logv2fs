{
    "log": {
        "level": "warn",
        "timestamp": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": false
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "proxyDns",
                "address": "https://8.8.8.8/dns-query",
                "detour": "manual-select"
            },
            {
                "tag": "localDns",
                "address": "https://223.5.5.5/dns-query",
                "detour": "direct"
            },
            {
                "tag": "block",
                "address": "rcode://success"
            },
            {
                "tag": "remote",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "domain": [
                    "ghproxy.com",
                    "cdn.jsdelivr.net",
                    "testingcf.jsdelivr.net"
                ],
                "server": "localDns"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "server": "block"
            },
            {
                "outbound": "any",
                "server": "localDns",
                "disable_cache": true
            },
            {
                "rule_set": "geosite-cn",
                "server": "localDns"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "server": "proxyDns"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "remote"
            }
        ],
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "independent_cache": true,
        "strategy": "ipv4_only"
    },
    "inbounds": [
        {
            "type": "tun",
            "address": [
                "172.19.0.1/30"
            ],
            "mtu": 9000,
            "auto_route": true,
            "strict_route": true,
            "endpoint_independent_nat": false,
            "stack": "mixed",
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 2080
                }
            }
        },
        {
            "type": "mixed",
            "listen": "127.0.0.1",
            "listen_port": 2080,
            "users": []
        }
    ],
    "outbounds": [
        {
            "tag": "manual-select",
            "type": "selector",
            "outbounds": [
                "auto"
            ]
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": [],
            "url": "http://www.gstatic.com/generate_204",
            "interval": "1m",
            "tolerance": 50
        },
        {
            "tag": "WeChat",
            "type": "selector",
            "outbounds": [
                "direct"
            ]
        },
        {
            "tag": "Apple",
            "type": "selector",
            "outbounds": [
                "direct"
            ]
        },
        {
            "tag": "Microsoft",
            "type": "selector",
            "outbounds": [
                "direct"
            ]
        },
        {
            "interval": "1m",
            "outbounds": [],
            "tag": "Openai",
            "tolerance": 50,
            "type": "urltest",
            "url": "http://www.gstatic.com/generate_204"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "final": "manual-select",
        "rules": [
            {
                "action": "sniff"
            },
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "network": "udp",
                "port": 443,
                "action": "reject"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "action": "reject"
            },
            {
                "domain": [
                    "w8.undervineyard.com",
                    "d.metacubex.one"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": "geosite-wechat",
                "outbound": "WeChat"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "outbound": "manual-select"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "rule_set": "geoip-cn",
                "outbound": "direct"
            },
            {
                "rule_set": "geosite-cn",
                "outbound": "direct"
            },
            {
                "rule_set": "geosite-apple",
                "outbound": "Apple"
            },
            {
                "rule_set": "geosite-microsoft",
                "outbound": "Microsoft"
            },
            {
                "outbound": "Openai",
                "rule_set": "geosite-openai"
            }
        ],
        "rule_set": [
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geoip/cn.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/cn.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/geolocation-!cn.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/category-ads-all.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-wechat",
                "type": "remote",
                "format": "source",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/wechat.json",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-apple",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/apple.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-microsoft",
                "type": "remote",
                "format": "binary",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/microsoft.srs",
                "download_detour": "direct"
            },
            {
                "download_detour": "direct",
                "format": "binary",
                "tag": "geosite-openai",
                "type": "remote",
                "url": "https://lllinter.oss-cn-hangzhou.aliyuncs.com/geo/geosite/openai.srs"
            }
        ]
    }
}